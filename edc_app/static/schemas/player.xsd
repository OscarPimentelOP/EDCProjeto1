      <xs:element type="xs:string" name="Age"/>
      <xs:element type="xs:string" name="PhotoUrl"/>
      <xs:element type="xs:string" name="TeamBadge"/>
      <xs:element type="xs:string" name="Height"/>
      <xs:element type="xs:string" name="Weight"/>
      <xs:element type="xs:string" name="Club"/>
      <xs:element type="xs:string" name="ClubPosition"/>
      <xs:element type="xs:string" name="ClubNumber"/>
      <xs:element type="xs:string" name="NationalTeam"/>
